<a href="/companies">Back to Companies</a>
<%= form_for @company do |f| %>
<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :description %>
  <%= f.text_field :description %>
</p>
<p>
  <%= f.label :location %>
  <%= f.text_field :location %>
</p>
<%= f.submit "submit" %>
<% end %>
